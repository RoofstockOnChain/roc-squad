  "bugs": {
    "url": "https://github.com/RoofstockOnChain/roc-squad/issues"
  },
  "homepage": "https://github.com/RoofstockOnChain/roc-squad#readme",
  "dependencies": {
    "@openzeppelin/contracts": "^4.6.0",
    "@truffle/hdwallet-provider": "^2.0.8",
    "dotenv": "^16.0.1",
    "truffle": "^5.5.15"
  },
  "devDependencies": {
    "truffle-plugin-verify": "^0.5.25"
  }
