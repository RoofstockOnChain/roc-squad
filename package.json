{
  "name": "roc-squad",
  "version": "1.0.0",
  "description": "An ERC-721 smart contract for the Roofstock onChain team.",
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "compile": "truffle compile",
